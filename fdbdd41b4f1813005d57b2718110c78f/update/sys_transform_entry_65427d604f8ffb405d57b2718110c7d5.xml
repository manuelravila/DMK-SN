        <source_script><![CDATA[answer = (function transformEntry(source, target, map, log, action) {

	// The Sync Ops Status for CMDB CI business rule keeps the install_status
	// and operational_status in sync. If you set one, without the other,
	// whatever you set might not take hold. Like only setting install_status
	// to "Retired" will result in an "Installed" transponder.
	// So set them both.
	if (source.u_component_status.toString() == "Active") {
		return "1"; // "Operational"
	} else if (source.u_component_status.toString() == "Dropped") {
//		return "6"; // "Retired"
		return "2"; // DOT Mod: Retired not in choice list
	}

})(source, target, map, log, action);]]></source_script>
