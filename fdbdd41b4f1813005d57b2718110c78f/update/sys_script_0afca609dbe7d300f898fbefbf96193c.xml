        <script><![CDATA[(function executeRule(current, previous /* null when async */) {
/**
 * @description Verify no other toll configuration records exist with the
 * same "nickname"
 */

	var currentCompany = current.getValue("company");

	var duplicateGR = new GlideRecord(current.getTableName());
	duplicateGR.addQuery("active", true);
	duplicateGR.addQuery("company", currentCompany);
	if (current.operation() == "update") {
		duplicateGR.addQuery("sys_id", "!=", current.sys_id);
	}
	duplicateGR.query();

	if (duplicateGR.hasNext()) {
		gs.addErrorMessage(
			gs.getMessage("Toll Configuration Duplicate Check-001", [
				current.getDisplayValue("company")
			])
		);
		current.setAbortAction(true);
	}

})(current, previous);
]]></script>
