        <script><![CDATA[function reassignTransponders() {
    var sysIds = g_list.getChecked();
    if (sysIds === '') {
        alert('Please select at least one transponder to reassign.');
        return;
    }

    var userReference = new GlideDialogWindow('x_89907_vehicle_to_user_reference_dialog');
    userReference.setTitle('Select User to Assign Transponders to');
    userReference.setPreference('sysparm_parent_id', sysIds);
    userReference.setSize(400, 400);
    userReference.render();
}
]]></script>
