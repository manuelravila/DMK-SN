        <source_script><![CDATA[answer = (function transformEntry(source) {

	// Add your code here
	var ep = source.u_entryplaza.toString();
	var ag = source.u_tollprovider.toString();
	
	var aggr = new GlideRecord("x_89907_vehicle_to_toll_agency");
	aggr.addQuery("code", ag);
	aggr.query();
	if (aggr.next()) {
		var epgr = new GlideRecord("x_89907_vehicle_to_toll_plaza");
		epgr.addQuery("agency", aggr.sys_id);
		epgr.addQuery("code", ep);
		epgr.query();
		if (epgr.next()) {
			return epgr.sys_id;
		}

	}


	return ""; // return the value to be put into the target field
	
})(source);]]></source_script>
