        <script><![CDATA[var VehicleCoordinatorUtilities = Class.create();
VehicleCoordinatorUtilities.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
	getCostCenters: function() {
		var costCenters = [];
		var cc = new GlideRecord('x_89907_vehicle_to_transponder_cost_center');
		cc.addQuery('vehicle_coordinators','CONTAINS',gs.getUserID());
		cc.query();
		while(cc.next()) {
			costCenters.push(cc.getValue('code'));
		}
		return costCenters;
	},
	
	type: 'VehicleCoordinatorUtilities'
});]]></script>
