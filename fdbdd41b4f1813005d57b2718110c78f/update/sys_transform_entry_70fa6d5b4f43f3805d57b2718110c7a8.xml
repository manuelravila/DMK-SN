        <source_script><![CDATA[answer = (function transformEntry(source) {

	if (source.u_component_status.toUpperCase() !== "DROPPED") {
		return "";
	}
	var srcDate;
	if (!source.u_removed_date.nil()) {
		srcDate = source.u_removed_date;
	} else if (!source.u_date_of_status.nil()) {
		srcDate = source.u_date_of_status;
	} else {
		return "";
	}
	
	var gdt = new GlideDateTime();
	gdt.setDisplayValue(srcDate + " 12:00:00", "dd-MMM-yy HH:mm:ss"); //uses current user session time zone and format from source
	var gd = new GlideDate(gdt.getDate());
	return gd.getByFormat("yyyy-MM-dd"); // must match date format specified above

})(source);]]></source_script>
