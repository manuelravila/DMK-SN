        <script><![CDATA[(function executeRule(current, previous /* null when async */) {
/** 
 * Present any info/error messages to the user that were generated by
 * UI actions like cancel, waive, etc.
 */

	if (current == null) {
		// prevent null pointer exceptions.
		return;
	}
	
	var messageList;
	messageList = current.internal_info_message;
	if (messageList) {
		addMessages(messageList, gs.addInfoMessage);
		current.internal_info_message = "";
	}

	messageList = current.internal_error_message;
	if (messageList) {
		addMessages(messageList, gs.addErrorMessage);
		current.internal_error_message = "";
	}
	
	function addMessages(messagesString, messageFn) {
		var messageArr = messagesString.split(",");
		for (var i = 0; i < messageArr.length; i++) {
			messageFn(messageArr[i]);
		}
	}
})(current, previous);]]></script>
