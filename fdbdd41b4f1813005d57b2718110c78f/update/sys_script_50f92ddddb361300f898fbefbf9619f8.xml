        <script><![CDATA[(function executeRule(current, previous /* null when async */) {
/**
 * @description Set the CI name for the transponder when saved.
 * CUSTOMIZE to suit your installation needs - this ID will be used in
 * and appear as the display value for the transponder in other ServiceNow
 * applications that rely on or reference the CMDB 
 * See also Transponder Replace business rule for display name-dependent logic
 */
	if (current == null) {
		// prevent null pointer exceptions.
		return;
	}
	if (current.u_transponder_number.changes()) {
		current.setValue("name", "EZ-" + current.u_transponder_number);
	}
})(current, previous);
]]></script>
