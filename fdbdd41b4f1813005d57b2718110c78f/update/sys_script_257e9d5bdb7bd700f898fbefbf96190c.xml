        <script><![CDATA[(function executeRule(current, previous /* null when async */) {
/** 
 * Set the display value for the toll statement
 */

	if (current == null) {
		// prevent null pointer exceptions.
		return;
	}
	if (
		!current.account.nil()
		&& current.account.changes() 
		&& !current.statement_date.nil() 
		&& current.statement_date.changes()) {
		current.display_value = current.account.account_number + " - " + current.statement_date;
	}
})(current, previous);]]></script>
