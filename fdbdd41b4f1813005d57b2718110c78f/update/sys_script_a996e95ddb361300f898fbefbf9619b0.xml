        <script><![CDATA[(function executeRule(current, previous /* null when async */) {
/**
 * Verify there are no other transponders with the same transponder number
 */
// todo should include vendor in this check?
	var columnName = "u_transponder_number";
	var transponderUtil = new Utilities();

	if (transponderUtil.DuplicateCheck(current, columnName) !== true) {
		return;
	}

	gs.addErrorMessage(
		gs.getMessage("Transponder Duplicate Check-001", [
			current.getValue(columnName)
		])
	);
	current.setAbortAction(true);

})(current, previous);
]]></script>
