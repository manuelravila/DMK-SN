        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	

	var gr= new GlideRecord('x_89907_vehicle_to_u_cmdb_ci_transponder');
	
	gr.addQuery('license_plate.license_number',current.license_plate);
	gr.addQuery('install_status',1);
	gr.addQuery('license_plate','!=','');
	gr.query();

	if(gr.hasNext() && current.license_plate != '')  {
		while (gr.next()) {
			gr.setValue('license_plate', '');
			//gs.addErrorMessage('The license plate '+ current.license_plate +' is in use by ' + gr.name + '. Please select a different license plate or deactivate the plate on the other transponder.'); // error message
			//current.setAbortAction(true);
			gr.update();
		}
	} else {	
		current.u_plate_number = previous.license_plate.getDisplayValue();
	} 
	

})(current, previous);]]></script>
